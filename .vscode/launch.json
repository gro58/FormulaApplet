        {
            "type": "chrome",
            "request": "launch",
            "name": "H5P Standalone - Launch Chrome from Wamp",
            "url": "http://localhost/h5p-standalone/",
            "webRoot": "${workspaceFolder}",
        },
        {
            "type": "chrome",
            "request": "launch",
            "name": "Launch Chrome from XAMPP",
            "url": "http://localhost:8080/gf09/sample_task_and_parse.php",
            "webRoot": "${workspaceFolder}",
        },
        {
            "type": "chrome",
            "request": "attach",
            "name": "Attach to Chrome",
            "port": 9222,
            "webRoot": "${workspaceFolder}"
        },
        {
            "type": "extensionHost",
            "request": "launch",
            "name": "Extension starten",
            "runtimeExecutable": "${execPath}",
            "args": [
                "--extensionDevelopmentPath=${workspaceFolder}"
            ],
            "outFiles": [
                "${workspaceFolder}/out/**/*.js"
            ],
            "preLaunchTask": "npm"
        },
        {
            "type": "chrome",
            "request": "launch",
            "name": "Launch Chrome against localhost",
            "url": "http://localhost",
            "webRoot": "${workspaceFolder}"
        }
